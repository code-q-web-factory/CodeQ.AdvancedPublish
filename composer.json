{
    "description": "The package adds a review workflow and change logs to the Neos publication function",
    "type": "neos-package",
    "name": "codeq/advancedpublish",
    "require": {
        "neos/neos": "^7 || ^8",
        "sandstorm/templatemailer": "^2",
        "neosidekick/revisions": "*"
    },
    "suggest": {
        "ext-bz2": "Install bz2 extension to compress changes content",
        "ext-igbinary": "Install igbinary for better serialization"
    },
    "autoload": {
        "psr-4": {
            "CodeQ\\AdvancedPublish\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "CodeQ.AdvancedPublish"
        }
    }
}
